{
	"name": "express-typescript-starter",
	"version": "1.0.0",
	"description": "Express.js starter template with TypeScript",
	"main": "dist/app.js",
	"type": "module",
	"scripts": {
		"start": "node dist/app.js",
		"dev": "npx tsx watch src/app.ts",
		"build": "tsc",
		"clean": "rimraf dist",
		"prebuild": "npm run clean"
	},
	"keywords": [
		"express",
		"typescript",
		"nodejs",
		"api"
	],
	"author": "James Chen",
	"license": "MIT",
	"dependencies": {
		"cors": "^2.8.5",
		"date-fns": "^4.1.0",
		"dotenv": "^16.3.1",
		"ejs": "^3.1.10",
		"express": "^4.18.2",
		"helmet": "^7.1.0",
		"morgan": "^1.10.0",
		"pg": "^8.16.3"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.10.0",
		"@types/pg": "^8.15.4",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.1",
		"tsx": "^4.20.3",
		"typescript": "^5.3.0"
	}
}
